# Makefile : Builds the helloapp application
#

# This demonstration program will error on link
#
cprog4.exe: cprog4.obj cprog4.def
	link /NOD /NOI /CO /ONERROR:NOEXE cprog4,,,safxcw libw slibcew,cprog4.def;
	rc /t cprog4.exe

cprog4.obj: cprog4.cpp
	cl /GA2s /Zpi /Od /c /W3 cprog4.cpp

clean:
	-del cprog4.exe
	-del cprog4.obj
	-del cprog4.map
