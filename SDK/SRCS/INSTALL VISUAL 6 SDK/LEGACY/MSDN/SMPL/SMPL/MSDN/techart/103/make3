# Makefile : Builds the helloapp application
#

# This demonstration program will error on link
#
cprog3.exe: cprog3.obj cprog3.def
	link /NOD /NOI /CO /ONERROR:NOEXE cprog3,,,safxcw libw slibcew,cprog3.def;
	rc /t cprog3.exe

cprog3.obj: cprog3.cpp
	cl /GA2s /Zpi /Od /c /W3 cprog3.cpp

clean:
	-del cprog3.exe
	-del cprog3.obj
	-del cprog3.map
