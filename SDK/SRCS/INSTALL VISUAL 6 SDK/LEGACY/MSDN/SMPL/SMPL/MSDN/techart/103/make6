# Makefile : Builds the helloapp application
#

# This demonstration program will error on link
#
cprog6.exe: cprog6.obj cprog6.def
	link /NOD /NOI /CO /ONERROR:NOEXE cprog6,,,safxcw libw slibcew,cprog6.def;
	rc /t cprog6.exe

cprog6.obj: cprog6.cpp
	cl /GA2s /Zpi /Od /c /W3 cprog6.cpp

clean:
	-del cprog6.exe
	-del cprog6.obj
	-del cprog6.map
