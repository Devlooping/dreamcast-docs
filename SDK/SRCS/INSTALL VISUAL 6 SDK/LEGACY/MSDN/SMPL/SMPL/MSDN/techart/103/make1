# Makefile : Builds the helloapp application
#

# This demonstration program will error on link
#
cprog1.exe: cprog1.obj cprog1.def
	link /NOD /NOI/CO /ONERROR:NOEXE cprog1,,,safxcw libw slibcew,cprog1.def;
	rc /t cprog1.exe

cprog1.obj: cprog1.cpp
	cl /GA2s /Zpi /Od /c /W3 cprog1.cpp

clean:
	-del cprog1.exe
	-del cprog1.obj
	-del cprog1.map
