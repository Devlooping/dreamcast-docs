go
set nocount on

IF EXISTS (SELECT * FROM sysobjects WHERE uid=user_id()
           AND      type='P'
           AND      name='pcGet_<!TbName!>')
    BEGIN
        PRINT '   > DROPPING - pcGet_<!TbName!> ...'
        DROP PROCEDURE pcGet_<!TbName!>
    END

go

PRINT 'CREATING      - pcGet_<!TbName!> ...'
go

CREATE PROCEDURE pcGet_<!TbName!>

-- File: 
-- Date: <!Date!>

@PKId       int

AS

SELECT *
FROM   <!TbName!>
WHERE  PKId = @PKId

go
