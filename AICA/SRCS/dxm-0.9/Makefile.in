# Makefile for dxm

@SET_MAKE@

srcdir:
	${MAKE} -C src

all:	srcdir testdir

clean:	cleandir

cleandir:
	${MAKE} -C src clean
	${MAKE} -C example clean

distclean:	distcleandir
	rm -f config.* Makefile

distcleandir:
	${MAKE} -C src distclean
	${MAKE} -C example distclean

test:	testdir

testdir:
	${MAKE} -C example test
